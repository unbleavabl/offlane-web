---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Offlane">
	<main>
		<img src="/public/logo-dark.svg" alt="logo" />
	</main>
</Layout>

<style>
	main {
		margin: auto;
		padding: 1.5rem;
		max-width: 60ch;
		min-height: calc(100vh - 3rem);
		display: flex;
		flex-direction: column;
		justify-content: center;
		align-items: center;
	}
</style>
